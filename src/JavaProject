public class Collection {
    
    private ArrayList<Usuario> listaUsuarios;

    public Collection() {
    listaUsuarios = new ArrayList<>();
    }    
    public boolean agregarUsuario(Usuario user1){
        return listaUsuarios.add(user1);
    }
    

   
    
   
    
    
    } 
    
    
    

    
